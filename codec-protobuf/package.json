{
  "name": "@aspect-ipc/codec-protobuf",
  "version": "0.0.0",
  "description": "Protocol Buffers codec for @aspect-ipc/transport.",
  "keywords": [
    "ipc",
    "protobuf",
    "protocol-buffers",
    "serialization",
    "codec",
    "binary",
    "node",
    "typescript"
  ],
  "author": "Sebastian Webdev",
  "repository": {
    "type": "git",
    "url": "https://github.com/SebastianWebdev/aspect-ipc.git",
    "directory": "codec-protobuf"
  },
  "bugs": {
    "url": "https://github.com/SebastianWebdev/aspect-ipc/issues"
  },
  "homepage": "https://github.com/SebastianWebdev/aspect-ipc/tree/main/codec-protobuf#readme",
  "type": "module",
  "sideEffects": false,
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist \"*.tsbuildinfo\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest run"
  },
  "dependencies": {
    "@aspect-ipc/transport": "workspace:*"
  },
  "peerDependencies": {
    "protobufjs": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "protobufjs": "^8.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  }
}
