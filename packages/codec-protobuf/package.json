{
  "name": "@procwire/codec-protobuf",
  "version": "0.2.3",
  "description": "Protocol Buffers codec for @procwire/transport.",
  "keywords": [
    "ipc",
    "protobuf",
    "protocol-buffers",
    "serialization",
    "codec",
    "binary",
    "node",
    "typescript"
  ],
  "author": "Sebastian Webdev",
  "repository": {
    "type": "git",
    "url": "https://github.com/SebastianWebdev/procwire.git",
    "directory": "packages/codec-protobuf"
  },
  "bugs": {
    "url": "https://github.com/SebastianWebdev/procwire/issues"
  },
  "homepage": "https://www.procwire.dev",
  "type": "module",
  "sideEffects": false,
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "clean": "rimraf dist \"*.tsbuildinfo\"",
