    "test": "vitest run"
  },
  "dependencies": {
    "@procwire/transport": "workspace:*"
  },
  "peerDependencies": {
    "protobufjs": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "protobufjs": "^8.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  }
